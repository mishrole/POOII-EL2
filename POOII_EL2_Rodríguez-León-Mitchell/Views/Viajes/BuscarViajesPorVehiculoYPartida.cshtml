@model IEnumerable<POOII_EL2_Rodríguez_León_Mitchell.Models.Viaje>

@{
    ViewBag.Title = "Buscar Viajes Por Vehiculo Y Partida";
}

<h2>Buscar Viajes Por Vehiculo Y Partida</h2>

@using (Html.BeginForm())
{
<div class="form-inline">
    <strong class="mr-5">Ingrese el código del vehículo</strong>
    @Html.TextBox("vehiculo", null, new { @class = "form-control col-sm-2 mt-4 mb-4 mr-5" })
    <strong class="mr-5">Ingrese el año:</strong>
    @Html.TextBox("partida", null, new { @class = "form-control col-sm-2 mt-4 mb-4 mr-5" })
    <button class="btn btn-primary">Buscar</button>
</div>
}
<!--    Embed PartialView   -->
@Html.Partial("_PartialViajes")
